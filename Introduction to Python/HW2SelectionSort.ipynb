   "source": [
    "#Selection Sort\n",
    "\n",
    "A = [64, 25, 12, 22, 11,44,2,47] \n",
    "\n",
    "for i in range(len(A)):\n",
    "    min_item=i\n",
    "    for x in range(i+1, len(A)):\n",
    "        if A[min_item]>A[x]:\n",
    "            min_item=x\n",
    "    #Swap\n",
    "    A[i],A[min_item]=A[min_item],A[i]\n",
    " \n",
    "#sorted list\n",
    "print(\"sorted list\")\n",
    "for i in A:\n",
    "    print(i)"
   ]
  }
